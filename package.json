    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0",
    "validator": "^13.0.0"
